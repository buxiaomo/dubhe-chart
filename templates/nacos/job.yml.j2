            username: {{ .Values.dubhe.mysql.username }}
            password: {{ .Values.dubhe.mysql.password }}
          props:
            sql:
              show: true
     
    management:
      health:
        db:
          enabled: false

  dubhe-algorithm.yaml: |
    train-algorithm:
