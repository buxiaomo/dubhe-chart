apiVersion: v1
kind: ConfigMap
metadata:
  name: dubhe
data:
  bootstrap-prod.yml: |-
    spring:
      cloud:
        nacos:
          username: nacos
          password: nacos
          context-path: /nacos
          config:
            namespace: {{ .Release.Namespace }}
            server-addr: nacos:8848
          discovery:
            namespace: {{ .Release.Namespace }}
            server-addr: nacos:8848
